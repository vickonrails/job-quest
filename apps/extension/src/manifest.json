{
    "name": "Job Quest",
    "version": "0.0.1",
    "description": "Chrome extension to copy jobs to Job Quest",
    "manifest_version": 3,
    "author": "Victor Ofoegbu",
    "action": {
        "default_popup": "index.html"
    },
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": ["https://linkedin.com/jobs/*"]
        }
    ]
}