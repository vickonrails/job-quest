{
  "name": "extension",
  "displayName": "Job Quest",
  "version": "0.0.1",
  "description": "Job description grabber for getjobquest.com. Operating system for your job search",
  "author": "Victor Ofoegbu. <victor@getjobquest.com>",
  "scripts": {
    "dev": "plasmo dev",
    "package": "plasmo package",
    "build": "plasmo build",
    "clean": "rm -rf build .plasmo node_modules .turbo"
  },
  "dependencies": {
    "@plasmohq/messaging": "0.5.0",
    "@plasmohq/storage": "1.7.2",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/supabase-js": "2.32.0",
    "@tiptap/pm": "^2.2.4",
    "@tiptap/react": "^2.2.4",
    "@tiptap/starter-kit": "^2.2.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.270.0",
    "plasmo": "0.82.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "shared": "workspace:*",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "ui": "workspace:*"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.0",
    "@plasmohq/parcel-resolver": "^0.14.0",
    "@types/chrome": "0.0.243",
    "@types/node": "20.5.0",
    "prettier": "3.0.2",
    "typescript": "5.1.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0"
  },
  "manifest": {
    "key": "$CRX_KEY",
    "permissions": [
      "tabs",
      "identity",
      "webNavigation"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/callback.html"
        ],
        "matches": [
          "<all_urls>"
        ],
        "extension_ids": [
          "$CRX_ID"
        ]
      }
    ]
  }
}
